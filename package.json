    "@material-ui/icons": "^3.0.1",
    "firebase": "^5.4.2",
    "moment": "^2.22.2",
    "parcel-bundler": "^1.9.7",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
